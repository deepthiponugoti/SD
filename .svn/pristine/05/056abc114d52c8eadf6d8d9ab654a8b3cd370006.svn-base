            assertEquals(1,fibonacciNumbers.getValueIterative(0));
        }

        @Test
        public void checkForTheFirstPositionOfTheSeriesForIterationFibonacci(){
            assertEquals(1, fibonacciNumbers.getValueIterative(1));
